[libs]

[lints]
all=warn
sketchy-null=off
untyped-import=off
unclear-type=off

[options]
include_warnings=true
module.ignore_non_literal_requires=true
traces=1
module.name_mapper='.*\(.pcss\)' -> 'empty/object'
suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
suppress_comment= \\(.\\|\n\\)*\\$FlowIssue
module.name_mapper='^src\/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
module.system=haste